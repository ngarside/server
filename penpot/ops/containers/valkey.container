[Container]
ContainerName=penpot_valkey
Image=ghcr.io/ngarside/valkey:9.0.0.36
Memory=8g
Network=penpot_internal.network:ip=10.4.0.2
NoNewPrivileges=true
PodmanArgs=--cpus=1
ReadOnly=true
UserNS=auto:size=8

[Service]
Restart=always
TimeoutStartSec=9000

[Install]
WantedBy=default.target
