{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["config:recommended"],
	"packageRules": [
		{
			"matchFiles": ["*"],
			"enabled": false
		},
		{
			"automerge": true,
			"automergeStrategy": "squash",
			"enabled": true,
			"matchFiles": ["requirements.txt"]
		}
	]
}
