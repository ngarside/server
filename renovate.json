{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["config:recommended"],
	"packageRules": [
		{
			"enabled": false,
			"matchFiles": ["*"]
		},
		{
			"automerge": true,
			"automergeStrategy": "squash",
			"enabled": true,
			"matchFiles": ["requirements.txt"]
		}
	],
	"prConcurrentLimit": 1
}
